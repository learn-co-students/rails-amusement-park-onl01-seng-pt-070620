<%= form_tag '/session', method: :post do %>
    <%= label_tag :name, "Enter your name:" %></br>

    <%= collection_select :user, :name, User.all, :name, :name, prompt: "Select your username" %></br>

    <%= label_tag :user_password, "Enter your password:" %>
    <%= password_field_tag :user_password, nil %></br>

    <%= submit_tag "Sign In" %>
<% end %>