<div class="alert">
<% if flash[:message] %>
    <h3><%= flash[:message] %></h3>
<% end %>
</div>

<% if current_user.admin %>
    <h1>ADMIN</h1>
<% end %>
<h1><%= current_user.name %></h1>

<p>Mood: <%= current_user.mood %></p>
<p>Tickets: <%= current_user.tickets %></p>
<p>Height: <%= current_user.height %></p>


<p><%= link_to 'See attractions', attractions_path %></p>